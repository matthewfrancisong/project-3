<div class = container>

<h1>Update Guest</h1>

<%= form_for @guest do |f| %>
  <%= f.label("Name:")%>
  <%= f.text_field :name %>
  <%= f.label("Email:")%>
  <%= f.text_field :email %>
  <%= f.label("RSVP:")%>
  <%= f.select :name, ([["Yes","Yes"],["No","No"]]) %>
  <%= f.submit("Update", class: 'btn') %>
<% end %>
</div>
